{
    "name": "graham-campbell/testbench-core",
    "description": "TestBench Core Provides Some Testing Functionality For Laravel",
    "keywords": ["laravel", "framework", "testing", "testbench-core", "TestBench", "Laravel TestBench Core", "Laravel-TestBench-Core", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        }
    ],
    "require": {
        "php": "^7.4.15 || ^8.0.2",
        "symfony/polyfill-php80": "^1.26"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^4.0",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "suggest": {
        "illuminate/support": "Required to use the laravel trait.",
        "mockery/mockery": "Required to use the mockery trait.",
        "phpunit/phpunit": "Required to use the most of the features."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\TestBenchCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\TestBenchCore\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
